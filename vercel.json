{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "env": {
        "GEMINI_API_KEY": "@gemini_api_key",
        "JIOCX_ORGANIZATION_NAME": "@jiocx_organization_name",
        "JIOCX_USERNAME": "@jiocx_username",
        "JIOCX_PASSWORD": "@jiocx_password",
        "JIOCX_API_KEY": "@jiocx_api_key",
        "JIOCX_SENDER_ID": "@jiocx_sender_id",
        "JIOCX_DLT_ENTITY_ID": "@jiocx_dlt_entity_id",
        "JIOCX_API_BASE_URL": "@jiocx_api_base_url"
    }
}